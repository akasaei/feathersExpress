{
  "name": "presonal",
  "description": "feathersExpress",
  "version": "0.0.0",
  "homepage": "",
  "private": true,
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "personal"
  },
  "contributors": [],
  "bugs": {},
  "engines": {
    "node": ">= 23.3.0"
  },
  "feathers": {
    "language": "js",
    "packager": "npm",
    "database": "mongodb",
    "framework": "express",
    "transports": [
      "rest"
    ],
    "schema": "json"
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "main": "lib/index",
  "type": "module",
  "scripts": {
    "start": "node src",
    "dev": "nodemon src/",
    "prettier": "npx prettier \"**/*.js\" --write",
    "mocha": "cross-env NODE_ENV=test mocha test/ --recursive --exit",
    "test": "npm run mocha",
    "bundle:client": "npm pack --pack-destination ./public"
  },
  "dependencies": {
    "@feathersjs/adapter-commons": "^5.0.34",
    "@feathersjs/authentication": "^5.0.34",
    "@feathersjs/authentication-client": "^5.0.34",
    "@feathersjs/authentication-local": "^5.0.34",
    "@feathersjs/authentication-oauth": "^5.0.34",
    "@feathersjs/configuration": "^5.0.34",
    "@feathersjs/errors": "^5.0.34",
    "@feathersjs/express": "^5.0.34",
    "@feathersjs/feathers": "^5.0.34",
    "@feathersjs/mongodb": "^5.0.34",
    "@feathersjs/schema": "^5.0.34",
    "@feathersjs/transport-commons": "^5.0.34",
    "@sentry/node": "^10.2.0",
    "@sentry/tracing": "^7.120.4",
    "compression": "^1.8.1",
    "config": "^4.1.0",
    "mongodb": "^6.16.0",
    "prom-client": "^15.1.3",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@feathersjs/cli": "^5.0.14",
    "@feathersjs/rest-client": "^5.0.34",
    "axios": "^1.9.0",
    "cross-env": "^10.0.0",
    "mocha": "^11.2.2",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2"
  }
}
